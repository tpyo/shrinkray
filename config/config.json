{
    "server_address": "0.0.0.0:9090",
    "management_address": "0.0.0.0:9091",
    "otel_collector_endpoint": "http://jaeger:4317",
    "read_timeout": 5,
    "log_level": "info",
    "proxies": [],
    "s3": {
        "access_key_id": "",
        "secret_access_key": "",
        "region": "us-east-1"
    },
    "routing": [
        {
            "path": "samples/{*path}",
            "endpoint": "https://shrinkray.photo/samples/"
        },
        {
            "path": "files/{*path}",
            "endpoint": "file:///app/files/"
        },
        {
            "path": "{*path}",
            "endpoint": "s3://bucket-name/"
        }
    ]
}
